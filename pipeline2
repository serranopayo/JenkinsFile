 node {
        checkout scm
        stage('Build'){
                echo 'Hola mundo'
                withMaven (
                  maven:'Maven text'
               ){   
                sh 'mvn compile'
                }
        }
        stage('Test'){
                echo 'Testing'
                sh 'mvn test'
        }
        stage('Deploy'){
                echo 'Deploying'
                sh 'mvn package'
        }

    }
    
